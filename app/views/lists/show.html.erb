<h1><%= @list.name %></h1>
<%= link_to "lists", lists_path %>
<%= link_to "+ add a movie", new_list_bookmark_path(@list) %>

<% @list.bookmarks.each do |bookmark| %>
  <h4><%= bookmark.movie.title %></h4>
  <%= link_to "Remove",
                  bookmark_path(bookmark),
                  method: :delete,
                  data: { confirm: "Are you sure?" } %>
<% end %>
